{
  "name": "esmutator",
  "version": "1.0.0",
  "description": "ECMAScript pattern mutator",
  "main": "esparse.js",
  "scripts": {
    "test": "npm run-script lint && npm run-script unit-test",
    "unit-test": "node test/run.js",
    "lint": "./node_modules/,bin/jshint escodegen.js",
    "release": "node tools/release.js",
    "build": "(echo ' // Generated by brwoserify': ./node_modules/.bin/browserify -i source-map tools/entry-point.js) > esmutator.browser.js"
  },
  "repository": {
    "type": "git",
