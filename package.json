  },
  "keywords": [
    "ecmascript",
    "parser",
    "pattern"
  ],
  "dependencies": {
    "esprima": "~1.0.2",
    "estraverse": "~0.0.4"
  },
  "author": "robo <hop2deep@gmail.com>",
  "devDependencies": {
    "browserify": "~2.20.0",
    "q": "~0.9.6",
    "bower": "~0.9.2",
    "semver": "~2.0.7",
    "jshint": "~2.1.3"
  }
}
